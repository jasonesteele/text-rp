{
  "version": "2.0.0",
  "command": "node_modules/.bin/tsc",
  "type": "shell",
  "args": ["--watch", "-p", "."],
  "echoCommand": true,
  "presentation": {
    "reveal": "silent"
  },
  "isBackground": true,
  "group": {
    "kind": "build",
    "isDefault": true
  },
  "problemMatcher": "$tsc-watch",
  "runOptions": { "runOn": "folderOpen" }
}
