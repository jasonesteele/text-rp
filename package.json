{
  "name": "yggdrasil",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "dev:test": "cross-env NODE_ENV=test next dev -- -p 3001",
    "db:migrate:dev": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/mydb?schema=public prisma migrate dev",
    "db:seed:dev": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/mydb?schema=public prisma db seed",
    "db:migrate:test": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5433/mydb?schema=public prisma migrate dev",
    "db:seed:test": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5433/mydb?schema=public prisma db seed",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "next start",
    "test": "echo Implement tests",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier -w .",
    "prettier:check": "prettier --check .",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.0",
    "@mui/system": "^5.8.0",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.14.0",
    "cross-fetch": "^3.1.5",
    "next": "12.1.6",
    "next-auth": "^4.3.4",
    "pino": "^7.11.0",
    "pino-http": "^7.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@cypress/skip-test": "^2.6.1",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@next/bundle-analyzer": "^12.1.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/node": "17.0.34",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "nock": "^13.2.4",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "ts-node": "^10.8.0",
    "typescript": "4.6.4"
  }
}
